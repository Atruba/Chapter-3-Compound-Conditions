#include <iostream>
#include <string>
using namespace std;

int main() {

	string answer;
	cout << "What is the capital of France?" << endl;
	cin >> answer;

	if (answer == "Paris" || answer == "paris")
		cout << "Your answer is correct!";

	else if (answer != "paris" && answer != "Paris") {
		cout << "Your answer is wrong.";
	}
	return 0;

}
